&#1074;&#1077;&#1088;&#1086;&#1091;&#1095;&#1077;&#1085;&#1080;&#1080;. &#1042; &#1089;&#1090;&#1088;&#1072;&#1085;&#1077; &#1089;&#1086;&#1079;&#1076;&#1072;&#1085;&#1099; &#1074;&#1089;&#1077; &#1091;&#1089;&#1083;&#1086;&#1074;&#1080;&#1103; &#1076;&#1083;&#1103; &#1087;&#1088;&#1080;&#1075;&#1083;&#1072;&#1096;&#1077;&#1085;&#1080;&#1103; &#1079;&#1072;&#1088;&#1091;&#1073;&#1077;&#1078;&#1085;&#1099;&#1093; 
&#1084;&#1080;&#1089;&#1089;&#1080;&#1086;&#1085;&#1077;&#1088;&#1086;&#1074; &#1076;&#1083;&#1103; &#1088;&#1072;&#1073;&#1086;&#1090;&#1099; &#1074; &#1089;&#1090;&#1088;&#1072;&#1085;&#1091; &#1080; &#1080;&#1093; &#1088;&#1077;&#1083;&#1080;&#1075;&#1080;&#1086;&#1079;&#1085;&#1086;&#1081; &#1076;&#1077;&#1103;&#1090;&#1077;&#1083;&#1100;&#1085;&#1086;&#1089;&#1090;&#1080;. &#1053;&#1072; 
&#1090;&#1077;&#1088;&#1088;&#1080;&#1090;&#1086;&#1088;&#1080;&#1080; &#1056;&#1077;&#1089;&#1087;&#1091;&#1073;&#1083;&#1080;&#1082;&#1080; &#1050;&#1072;&#1079;&#1072;&#1093;&#1089;&#1090;&#1072;&#1085; &#1084;&#1080;&#1089;&#1089;&#1080;&#1086;&#1085;&#1077;&#1088;&#1089;&#1082;&#1086;&#1081; &#1076;&#1077;&#1103;&#1090;&#1077;&#1083;&#1100;&#1085;&#1086;&#1089;&#1090;&#1100;&#1102; &#1084;&#1086;&#1075;&#1091;&#1090; 
&#1079;&#1072;&#1085;&#1080;&#1084;&#1072;&#1090;&#1100;&#1089;&#1103; &#1075;&#1088;&#1072;&#1078;&#1076;&#1072;&#1085;&#1077; &#1056;&#1077;&#1089;&#1087;&#1091;&#1073;&#1083;&#1080;&#1082;&#1080; &#1050;&#1072;&#1079;&#1072;&#1093;&#1089;&#1090;&#1072;&#1085;, &#1080;&#1085;&#1086;&#1089;&#1090;&#1088;&#1072;&#1085;&#1094;&#1099; &#1080; &#1083;&#1080;&#1094;&#1072; &#1073;&#1077;&#1079; 
