&#1085;&#1077;&#1087;&#1088;&#1080;&#1085;&#1103;&#1090;&#1080;&#1103; &#1080;&#1084;&#1080; &#1088;&#1072;&#1076;&#1080;&#1082;&#1072;&#1083;&#1100;&#1085;&#1099;&#1093; &#1088;&#1077;&#1083;&#1080;&#1075;&#1080;&#1086;&#1079;&#1085;&#1099;&#1093; &#1080;&#1076;&#1077;&#1081;. &#1054;&#1073;&#1098;&#1077;&#1084; &#1091;&#1095;&#1077;&#1073;&#1085;&#1086;&#1081; &#1085;&#1072;&#1075;&#1088;&#1091;&#1079;&#1082;&#1080; 
&#1089;&#1086;&#1089;&#1090;&#1072;&#1074;&#1083;&#1103;&#1077;&#1090; 34 &#1095;&#1072;&#1089;&#1072; &#1074; &#1075;&#1086;&#1076; &#1080;&#1083;&#1080; &#1087;&#1086; &#1086;&#1076;&#1085;&#1086;&#1084;&#1091; &#1072;&#1082;&#1072;&#1076;&#1077;&#1084;&#1080;&#1095;&#1077;&#1089;&#1082;&#1086;&#1084;&#1091; &#1095;&#1072;&#1089;&#1091; &#1074; &#1085;&#1077;&#1076;&#1077;&#1083;&#1102;. 
&#1053;&#1077;&#1086;&#1073;&#1093;&#1086;&#1076;&#1080;&#1084;&#1086; &#1086;&#1090;&#1084;&#1077;&#1090;&#1080;&#1090;&#1100;, &#1095;&#1090;&#1086; &#1096;&#1082;&#1086;&#1083;&#1100;&#1085;&#1099;&#1081; &#1091;&#1095;&#1077;&#1073;&#1085;&#1080;&#1082; &#171;&#1054;&#1089;&#1085;&#1086;&#1074;&#1099; &#1088;&#1077;&#1083;&#1080;&#1075;&#1080;&#1086;&#1074;&#1077;&#1076;&#1077;&#1085;&#1080;&#1103;&#187; 
