&#1085;&#1072;&#1089;&#1077;&#1083;&#1103;&#1102;&#1097;&#1080;&#1093; &#1077;&#1075;&#1086; &#1085;&#1072;&#1088;&#1086;&#1076;&#1086;&#1074;. &#1050;&#1088;&#1086;&#1084;&#1077; &#1090;&#1086;&#1075;&#1086;, &#1074; &#1079;&#1072;&#1082;&#1086;&#1085;&#1086;&#1076;&#1072;&#1090;&#1077;&#1083;&#1100;&#1089;&#1090;&#1074;&#1072;&#1093; &#1087;&#1086;&#1095;&#1090;&#1080; &#1074;&#1089;&#1077;&#1093; &#1089;&#1090;&#1088;&#1072;&#1085; 
&#1057;&#1053;&#1043; (&#1056;&#1086;&#1089;&#1089;&#1080;&#1103;, &#1041;&#1077;&#1083;&#1086;&#1088;&#1091;&#1089;&#1089;&#1080;&#1103;, &#1043;&#1088;&#1091;&#1079;&#1080;&#1103;, &#1058;&#1072;&#1076;&#1078;&#1080;&#1082;&#1080;&#1089;&#1090;&#1072;&#1085;, &#1058;&#1091;&#1088;&#1082;&#1084;&#1077;&#1085;&#1080;&#1089;&#1090;&#1072;&#1085;, &#1051;&#1080;&#1090;&#1074;&#1072;) 
&#1079;&#1072;&#1082;&#1088;&#1077;&#1087;&#1083;&#1103;&#1077;&#1090;&#1089;&#1103; &#1090;&#1088;&#1072;&#1076;&#1080;&#1094;&#1080;&#1086;&#1085;&#1085;&#1099;&#1081; &#1089;&#1090;&#1072;&#1090;&#1091;&#1089; &#1086;&#1087;&#1088;&#1077;&#1076;&#1077;&#1083;&#1077;&#1085;&#1085;&#1099;&#1093; &#1082;&#1086;&#1085;&#1092;&#1077;&#1089;&#1089;&#1080;&#1081;, &#1072; &#1074; &#1040;&#1088;&#1084;&#1077;&#1085;&#1080;&#1080; 
