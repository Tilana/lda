&#1085;&#1077;&#1079;&#1072;&#1082;&#1086;&#1085;&#1085;&#1086;&#1081; &#1076;&#1077;&#1103;&#1090;&#1077;&#1083;&#1100;&#1085;&#1086;&#1089;&#1090;&#1080; &#1089;&#1077;&#1082;&#1090;&#187; (&#8470;1412 &#1086;&#1090; 1999 &#1075;.) &#1080; &#171;&#1054; &#1089;&#1077;&#1082;&#1090;&#1072;&#1093; &#1080; &#1085;&#1086;&#1074;&#1099;&#1093; 
&#1088;&#1077;&#1083;&#1080;&#1075;&#1080;&#1086;&#1079;&#1085;&#1099;&#1093; &#1076;&#1074;&#1080;&#1078;&#1077;&#1085;&#1080;&#1103;&#1093;&#187; (&#8470;1178 &#1086;&#1090; 1992&#1075;.). &#1055;&#1088;&#1080; &#1101;&#1090;&#1086;&#1084; &#1056;&#1077;&#1082;&#1086;&#1084;&#1077;&#1085;&#1076;&#1072;&#1094;&#1080;&#1080; 
&#1085;&#1072;&#1087;&#1088;&#1072;&#1074;&#1083;&#1077;&#1085;&#1099; &#1085;&#1072; &#171;&#1079;&#1072;&#1097;&#1080;&#1090;&#1091; &#1086;&#1090; &#1072;&#1090;&#1072;&#1082; &#1089;&#1077;&#1082;&#1090; &#1080; &#1085;&#1077;&#1090;&#1088;&#1072;&#1076;&#1080;&#1094;&#1080;&#1086;&#1085;&#1085;&#1099;&#1093; &#1088;&#1077;&#1083;&#1080;&#1075;&#1080;&#1086;&#1079;&#1085;&#1099;&#1093; &#1075;&#1088;&#1091;&#1087;&#1087; &#1085;&#1072; 
