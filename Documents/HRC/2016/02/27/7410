&#1085;&#1072;&#1082;&#1072;&#1079;&#1072;&#1085;&#1080;&#1102; &#1083;&#1080;&#1096;&#1100; &#1079;&#1072; &#1090;&#1086;, &#1095;&#1090;&#1086; &#1086;&#1085;&#1080; &#1075;&#1086;&#1074;&#1086;&#1088;&#1080;&#1083;&#1080; &#1086; 
&#1089;&#1074;&#1086;&#1077;&#1081; &#1074;&#1077;&#1088;&#1077; &#1080;&#1083;&#1080; &#1087;&#1091;&#1073;&#1083;&#1080;&#1095;&#1085;&#1086; &#1086;&#1090;&#1074;&#1077;&#1095;&#1072;&#1083;&#1080; &#1085;&#1072; 
&#1074;&#1086;&#1087;&#1088;&#1086;&#1089;&#1099;. &#1057;&#1086;&#1086;&#1073;&#1097;&#1072;&#1077;&#1090;&#1089;&#1103;, &#1095;&#1090;&#1086; &#1080;&#1085;&#1086;&#1075;&#1076;&#1072; &#1090;&#1077;&#1088;&#1084;&#1080;&#1085; 
&#171;&#1084;&#1080;&#1089;&#1089;&#1080;&#1086;&#1085;&#1077;&#1088;&#1089;&#1082;&#1072;&#1103; &#1076;&#1077;&#1103;&#1090;&#1077;&#1083;&#1100;&#1085;&#1086;&#1089;&#1090;&#1100;&#187; &#1080;&#1089;&#1087;&#1086;&#1083;&#1100;&#1079;&#1091;&#1077;&#1090;&#1089;&#1103; 
&#1085;&#1072;&#1089;&#1090;&#1086;&#1083;&#1100;&#1082;&#1086; &#1096;&#1080;&#1088;&#1086;&#1082;&#1086;, &#1095;&#1090;&#1086;&#1073;&#1099; &#1080;&#1084; &#1084;&#1086;&#1078;&#1085;&#1086; &#1073;&#1099;&#1083;&#1086; 
